{
  "name": "discordjs-docs-parser",
  "version": "1.3.1",
  "workspaces": [
    "documentation/"
  ],
  "description": "Documentation parser for DiscordJS projects",
  "author": "favna",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "sideEffects": false,
  "homepage": "https://discordjs-docs-parser.vercel.app",
  "scripts": {
    "lint": "eslint src tests documentation/src --ext mjs,js,ts,tsx --fix",
    "format": "prettier --write \"{src,tests,documentation/src}/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "update": "yarn upgrade-interactive",
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "node scripts/clean.mjs",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run",
    "postinstall": "husky install .github/husky",
    "prepack": "yarn build && pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/utilities": "^3.11.0",
    "@skyra/jaro-winkler": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@favware/cliff-jumper": "^1.9.0",
    "@sapphire/docusaurus-plugin-npm2yarn2pnpm": "^1.1.4",
    "@sapphire/eslint-config": "^4.3.8",
    "@sapphire/prettier-config": "^1.4.4",
    "@sapphire/ts-config": "^3.3.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitest/coverage-c8": "^0.26.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vitest": "^0.26.2"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "minimist": "^1.2.7",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favna/discordjs-docs-parser.git"
  },
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ],
  "engines": {
    "node": ">=v16",
    "npm": ">=7"
  },
  "keywords": [
    "discordjs",
    "favna",
    "docs",
    "doc parser",
    "bot",
    "typescript",
    "ts",
    "yarn",
    "discord"
  ],
  "bugs": {
    "url": "https://github.com/favna/discordjs-docs-parser/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.3.1"
}
