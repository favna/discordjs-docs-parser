{
  "name": "discordjs-docs-parser",
  "version": "0.0.1",
  "workspaces": [
    "documentation/"
  ],
  "description": "Documentation parser for DiscordJS projects",
  "author": "favna",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "sideEffects": false,
  "homepage": "https://discordjs-docs-parser.vercel.app",
  "scripts": {
    "lint": "eslint src tests documentation/src --ext mjs,js,ts,tsx --fix",
    "format": "prettier --write {src,tests}/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "update": "yarn upgrade-interactive",
    "build": "tsc -b src && gen-esm-wrapper dist/index.js dist/index.mjs",
    "watch": "tsc -b src -w",
    "clean": "tsc -b src --clean",
    "sversion": "standard-version",
    "prepublishOnly": "yarn build",
    "prepare": "husky install .github/husky"
  },
  "dependencies": {
    "@sapphire/fetch": "^2.0.3",
    "common-tags": "^1.8.2",
    "fuse.js": "^6.4.6",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@sapphire/eslint-config": "^4.0.5",
    "@sapphire/prettier-config": "^1.2.4",
    "@sapphire/ts-config": "^3.1.4",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.8",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.13.14",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-esm-wrapper": "^1.1.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-circus": "^27.3.1",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favna/discordjs-docs-parser.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=v16.6.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "discordjs",
    "favna",
    "docs",
    "doc parser",
    "bot",
    "typescript",
    "ts",
    "yarn",
    "discord"
  ],
  "bugs": {
    "url": "https://github.com/favna/discordjs-docs-parser/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.1.0"
}
